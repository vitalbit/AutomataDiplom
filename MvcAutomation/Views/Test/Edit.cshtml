@{
    ViewBag.Title = "Edit";
}

<div style="margin-top: 10px">
    <div style="width: 240px; float: left">
        <center><a href="" class="add-js-file edit-icon" title="Добавить JS файл"></a></center>
        <input type="text" name="jsFile" readonly />
        <br />
        <br />
        <center><a href="" class="add-css-file edit-icon" title="Добавить CSS файл"></a></center>
        <input type="text" name="cssFile" readonly />
    </div>
    <div class="edit-test-arrow edit-icon"></div>
    <div style="float: left; width: 200px; margin: 110px 0 0 20px">
        <input type="text" name="testType" placeholder="Тип теста" />
